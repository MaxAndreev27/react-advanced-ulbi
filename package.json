    },
    "dependencies": {
        "@headlessui/react": "^2.2.0",
        "@reduxjs/toolkit": "^2.5.1",
        "@use-gesture/react": "^10.3.1",
        "axios": "^1.7.9",
        "concurrently": "^9.1.2",
        "i18next": "^24.2.1",
        "i18next-browser-languagedetector": "^8.0.2",
        "i18next-http-backend": "^3.0.1",
        "react": "^19.0.0",
        "react-device-detect": "^2.2.3",
        "react-dom": "^19.0.0",
        "react-i18next": "^15.4.0",
        "react-redux": "^9.2.0",
        "react-router-dom": "^7.1.1",
        "react-spring-web-modified": "^1.0.0"
    },
    "loki": {
        "configurations": {
            "chrome.laptop": {
                "target": "chrome.docker",
                "width": 1366,
                "height": 768,
                "deviceScaleFactor": 1,
                "mobile": false
            },
            "chrome.iphone7": {
                "target": "chrome.docker",
                "preset": "iPhone 7"
            }
        }
    }
